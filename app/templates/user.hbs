<p>Welcome {{model.userprofile.userName}}</p>
{{log model.userprofile}}
{{user-profile updateUserName="updateUserName" userprofile=model.userprofile}}
<div class="col sm6">
  <h5>{{new-question save2="saveQuestion" session=session}}</h5>
</div>
{{show-user model.userprofile.userName}}
<h3 class="center">The Questions</h3>
<hr>
<div class="questions">
  {{#each model.questions as |question|}}
  {{question-tile question=question update="update" destroyQuestion="destroyQuestion" username=model.userprofile.userName}}
  {{/each}}
</div>
